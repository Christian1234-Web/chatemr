{
	"name": "emr",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "yarn scss && react-scripts start",
		"scss": "node-sass src/assets/scss/main.scss src/assets/css/main.css --source-map true",
		"scss:watch": "yarn scss && node-sass src/assets/scss/main.scss src/assets/css/main.css -rw --source-map true",
		"scss:build": "node-sass -r --output-style compressed src/assets/scss/main.scss -o src/assets/css",
		"build": "yarn clean && yarn scss:build && react-scripts build",
		"test": "react-scripts test",
		"clean": "rm -rf build",
		"eject": "react-scripts eject"
	},
