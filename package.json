    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^3.26.8",
    "axios": "^0.19.2",
    "jquery": "^3.4.1",
    "lodash.capitalize": "^4.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.orderby": "^4.6.0",
    "lodash.size": "^4.2.0",
    "lodash.split": "^4.4.2",
    "lodash.startcase": "^4.4.0",
    "lodash.template": "^4.5.0",
    "lodash.uppercase": "^4.3.0",
